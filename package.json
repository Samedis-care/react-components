		"eslint-plugin-makestyles": "^0.1.11",
		"eslint-plugin-prettier": "^3.3.0",
		"husky": "^5.0.9",
		"install-peers-cli": "^2.2.0",
		"lint-staged": "^10.5.3",
		"prettier": "^2.2.1",
		"typedoc": "^0.22.15"
	},
	"engines": {
		"npm": ">=7.x"
	}
}
