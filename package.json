{
	"name": "components-care",
	"version": "0.1.0",
	"main": "dist",
	"types": "dist",
	"repository": "https://github.com/Components-care/react-components",
	"license": "Apache-2.0",
	"dependencies": {
		"@date-io/moment": "^1.3.13",
		"@material-ui/core": "^4.11.2",
		"@material-ui/icons": "^4.11.2",
		"@material-ui/lab": "^4.0.0-alpha.57",
		"@material-ui/pickers": "^3.2.10",
		"@types/node": "^14.14.14",
		"@types/react-signature-canvas": "^1.0.1",
		"@types/xlsx": "^0.0.36",
		"i18next": "^19.8.4",
		"i18next-browser-languagedetector": "^6.0.1",
		"i18next-resource-store-loader": "^0.1.2",
		"libphonenumber-js": "^1.9.6",
		"moment": "^2.29.1",
		"react-color": "^2.19.3",
		"react-i18next": "^11.8.11",
		"react-query": "^3.5.1",
		"react-scripts": "^4.0.1",
		"react-signature-canvas": "^1.0.3",
		"react-virtualized": "^9.22.3",
		"typescript": "^4.2.2",
		"xlsx": "^0.17.3"
	},
	"peerDependencies": {
		"@sentry/react": "^6.12.0",
		"@sentry/tracing": "^6.12.0",
		"@types/react": "^17.0.1",
		"@types/react-dom": "^17.0.1",
		"@types/react-router-dom": "^5.1.0",
		"react": "^17.0.1",
		"react-dom": "^17.0.1",
		"react-router-dom": "^5.1.0"
	},
	"peerDependenciesMeta": {
		"@types/react": {
			"optional": true
		},
		"@types/react-dom": {
			"optional": true
		},
		"@types/react-router-dom": {
			"optional": true
		},
		"@sentry/react": {
			"optional": true
		},
		"@sentry/tracing": {
			"optional": true
		}
	},
	"scripts": {
		"docgen": "typedoc --exclude 'src/stories/**/*.*' --exclude 'node_modules/**/*.*' --out typedoc --entryPoints src/index.ts",
		"start": "npm run storybook",
		"prestart": "husky install",
		"build": "tsc && cpx 'src/assets/**/*' dist/assets",
		"storybook": "start-storybook -p 9009",
		"build-storybook": "build-storybook",
		"build-ghpages": "bash build-ghpages.sh",
		"deploy-storybook": "storybook-to-ghpages -o storybook-static -s build-ghpages",
		"lint": "eslint 'src/**/*.{ts,tsx}'",
		"lint-fix": "eslint --fix 'src/**/*.{ts,tsx}'",
		"test": "TZ=UTC jest"
	},
	"sideEffects": false,
	"eslintConfig": {
		"extends": "react-app"
	},
	"lint-staged": {
		"(src|test)/**/*.{js,jsx,ts,tsx}": "eslint --max-warnings=0 --fix",
		"*.json": "prettier --write"
	},
	"jest": {
		"setupFiles": [
			"jest-date-mock"
		],
		"transform": {
			".(ts|tsx)": "ts-jest"
		},
		"testPathIgnorePatterns": [
			"/node_modules/",
			"/docs/",
			"/dist/"
		],
		"testRegex": "/test/.*?\\.(test|spec)\\.(ts|tsx|js)$",
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js",
			"json"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@babel/cli": "^7.12.10",
		"@babel/core": "^7.12.10",
		"@babel/plugin-proposal-class-properties": "^7.12.1",
		"@babel/preset-env": "^7.12.11",
		"@babel/preset-react": "^7.12.10",
		"@babel/preset-typescript": "^7.12.7",
		"@storybook/addon-actions": "^6.1.11",
		"@storybook/addon-knobs": "^6.1.11",
		"@storybook/addon-links": "^6.1.11",
		"@storybook/addon-storyshots": "^6.1.11",
		"@storybook/addons": "^6.1.11",
		"@storybook/preset-create-react-app": "^3.1.5",
		"@storybook/react": "^6.1.11",
		"@storybook/storybook-deployer": "^2.8.7",
		"@testing-library/jest-dom": "^5.11.6",
		"@testing-library/react": "^11.2.2",
		"@testing-library/user-event": "^12.6.0",
		"@types/enzyme": "^3.10.8",
		"@types/jest": "^26.0.19",
		"@types/react-virtualized": "^9.21.10",
		"@wojtekmaj/enzyme-adapter-react-17": "^0.4.1",
		"babel-jest": "^26.6.3",
		"cpx": "^1.5.0",
		"enzyme": "^3.11.0",
		"enzyme-to-json": "^3.6.1",
		"eslint-config-prettier": "^7.1.0",
		"eslint-plugin-makestyles": "^0.1.11",
		"eslint-plugin-prettier": "^3.3.0",
		"husky": "^5.0.9",
		"install-peers-cli": "^2.2.0",
		"jest": "^26.6.3",
		"jest-date-mock": "^1.0.8",
		"jest-fetch-mock": "^3.0.3",
		"lint-staged": "^10.5.3",
		"prettier": "^2.2.1",
		"ts-jest": "^26.4.4",
		"typedoc": "^0.20.29"
	},
	"engines": {
		"npm": ">=7.x"
	}
}
